<body>
    <script>
        function sumOfPositiveNo(numbers) {
            let sum = 0;
            for (let i = 1; i < numbers.length; i++) {
                if (numbers[i] > 0) {
                    sum += numbers[i]
                }
            }
            return sum
        }
        const array = [5, 6, 7, 8, 2, 8, 9, 4]

        console.log(sumOfPositiveNo(array))
    </script>
</body>

</html>
