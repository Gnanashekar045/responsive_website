<body>
    <script>
        function secondHighestNo(arr) {
            const sortedarray = arr.sort((a, b) => b - a)
            return sortedarray[1]
        }

        const numbers = [4,3,6,7,9,3,4,6]

        console.log(secondHighestNo(numbers))
    </script>
</body>

</html>
