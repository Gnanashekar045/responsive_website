<body>
    <script>
        function removeDuplicates(arr) {
            return [...new Set(arr)]
        }

        const array = [4,6,7,7,8,4,2,6,7,3,6,4,6,7,7,8]

        console.log(removeDuplicates(array))
    </script>
</body>

